<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".components.CurrentWeatherFragment">


    <TextView
            android:layout_width="283dp"
            android:layout_height="36dp"
            android:id="@+id/windTextView"
            android:textAlignment="viewStart" android:textSize="20sp"
            android:textColor="#000000"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="319dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="376dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="69dp" android:layout_marginEnd="59dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:id="@+id/locationTextView"
            android:textColor="#000000" android:textSize="28sp"
            android:textAlignment="center"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="82dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="603dp"/>
    <TextView
            android:layout_width="164dp"
            android:layout_height="93dp"
            android:id="@+id/forecastTemperatureTextView" android:textColor="#000000"
            android:textSize="50sp" android:textAlignment="center"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="138dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="208dp" android:layout_marginEnd="39dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="500dp"/>
    <TextView
            android:layout_width="186dp"
            android:layout_height="50dp"
            android:id="@+id/weatherTextView"
            android:textAlignment="viewStart" android:textSize="20sp"
            android:textColor="#000000"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="230dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="451dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="208dp" android:layout_marginEnd="17dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="283dp"
            android:layout_height="36dp"
            android:id="@+id/pressureTextView"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="378dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="69dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="317dp"
            android:layout_marginEnd="59dp" app:layout_constraintEnd_toEndOf="parent"
            android:textAlignment="viewStart" android:textColor="#000000" android:textSize="20dp"/>
    <TextView
            android:layout_width="283dp"
            android:layout_height="36dp"
            android:id="@+id/humidityTextView" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="443dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="252dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="69dp" android:layout_marginEnd="59dp"
            app:layout_constraintEnd_toEndOf="parent" android:textSize="20sp" android:textColor="#000000"/>
</androidx.constraintlayout.widget.ConstraintLayout>