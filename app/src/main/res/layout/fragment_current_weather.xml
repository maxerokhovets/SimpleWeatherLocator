<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".components.CurrentWeatherFragment">


    <ImageView
            android:layout_width="74dp"
            android:layout_height="79dp"
            android:id="@+id/windImageView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="317dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="335dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" android:layout_marginEnd="239dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="170dp"
            android:layout_height="40dp"
            android:id="@+id/windTextView"
            android:textAlignment="viewStart" android:textSize="25sp"
            android:textColor="#000000"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="337dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="354dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="172dp" android:layout_marginEnd="69dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:id="@+id/locationTextView"
            android:textColor="#000000" android:textSize="28sp"
            android:textAlignment="center"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="82dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="603dp"/>
    <TextView
            android:layout_width="164dp"
            android:layout_height="93dp"
            android:id="@+id/forecastTemperatureTextView" android:textColor="#000000"
            android:textSize="50sp" android:textAlignment="center"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="138dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="208dp" android:layout_marginEnd="39dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="500dp"/>
    <TextView
            android:layout_width="186dp"
            android:layout_height="50dp"
            android:id="@+id/weatherTextView"
            android:textAlignment="viewStart" android:textSize="20sp"
            android:textColor="#000000"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="230dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="451dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="208dp" android:layout_marginEnd="17dp"
            app:layout_constraintEnd_toEndOf="parent" android:paddingStart="5dp"/>
    <TextView
            android:layout_width="170dp"
            android:layout_height="40dp"
            android:id="@+id/pressureTextView"
            android:textAlignment="viewStart" android:textColor="#000000" android:textSize="25dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="492dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="199dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="172dp" android:layout_marginEnd="69dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="170dp"
            android:layout_height="40dp"
            android:id="@+id/humidityTextView"
            android:textSize="25sp" android:textColor="#000000"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="568dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="123dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="172dp" android:layout_marginEnd="69dp"
            app:layout_constraintEnd_toEndOf="parent" android:paddingStart="3dp"/>
    <ImageView
            android:layout_width="193dp"
            android:layout_height="185dp"
            android:id="@+id/currentWeatherImageView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="106dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="440dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginEnd="202dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="170dp"
            android:layout_height="40dp"
            android:id="@+id/windDirectionTextView"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="415dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="172dp"
            android:layout_marginEnd="69dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="276dp"
            android:textAlignment="viewStart" android:textColor="#000000" android:textSize="25sp"/>
    <ImageView
            android:layout_width="74dp"
            android:layout_height="79dp"
            android:id="@+id/windDirectionImageView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="396dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="256dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" android:layout_marginEnd="239dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="72dp"
            android:layout_height="73dp"
            android:id="@+id/pressureImageView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="476dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="182dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="101dp" android:layout_marginEnd="238dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="74dp"
            android:layout_height="79dp"
            android:id="@+id/humidityImageView" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="549dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="103dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="99dp" android:layout_marginEnd="238dp"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>